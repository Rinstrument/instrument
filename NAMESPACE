# Edit by hand (not managed by Roxygen)

# Main model fitting function of package
export(theta2)

# Rcpp
import(Rcpp)

# Methods
import(methods)

# Use compiled code (Stan -> cpp -> theta2)
useDynLib(theta2, .registration = TRUE)

# S3 method exports
S3method(summary, theta2Obj)
export(summary.theta2Obj)
S3method(traceplot, theta2Obj)
export(traceplot.theta2Obj)

# From rstan
importFrom(rstan, sampling)
importFrom(rstan, extract)
importFrom(rstan, traceplot)
importFrom(rstan, vb)

# from data.table
importFrom(data.table, transpose)
importFrom(data.table, dcast)
importFrom(data.table, as.data.table)

# from purrr
importFrom(purrr, map)

# from stringr
# import(stringr)
importFrom(stringr, str_split)
importFrom(stringr, str_replace_all)
importFrom(stringr, str_detect)
importFrom(stringr, str_subset)
importFrom(stringr, str_squish)
importFrom(stringr, str_remove_all)
